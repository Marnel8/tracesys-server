{
	"name": "express-server",
	"version": "1.2.0",
	"description": "A final server for TRIOE website",
	"main": "index.js",
	"type": "commonjs",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "tsx watch -r tsconfig-paths/register  src/index.ts",
		"start": "node dist/index.js",
		"ext": "ts",
		"build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json --resolve-full-paths && pnpm run copy-assets",
		"copy-assets": "copyfiles -u 1 \"src/uploads/**/*\" \"dist/\" && copyfiles -u 1 \"src/assets/**/*\" \"dist/\" && copyfiles -u 1 \"src/mails/**/*\" \"dist/\""
	},
	"author": "Batangas State University",
	"license": "ISC",
	"dependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/cookie-parser": "^1.4.8",
		"@types/cors": "^2.8.17",
		"@types/ejs": "^3.1.5",
		"@types/express": "^5.0.0",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/multer": "^1.4.12",
		"@types/nodemailer": "^6.4.17",
		"bcryptjs": "^2.4.3",
		"colors": "^1.4.0",
		"cookie-parser": "^1.4.7",
		"copyfiles": "^2.4.1",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"ejs": "^3.1.10",
		"express": "^5.0.0",
		"http-status-codes": "^2.3.0",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"mysql2": "^3.11.5",
		"nodemailer": "^6.9.16",
		"reflect-metadata": "^0.2.2",
		"sequelize": "^6.37.5",
		"sequelize-typescript": "^2.1.6",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0"
	},
	"devDependencies": {
		"@types/morgan": "^1.9.9",
		"@types/node": "^22.10.5",
		"@types/validator": "^13.12.2",
		"tsc-alias": "^1.8.10",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3"
	}
}
